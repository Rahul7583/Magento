<?xml version="1.0"?>
<layout>
	<process_adminhtml_process_index>
		<reference name="content">
			<block type="process/adminhtml_process_index" name="process_grid"/>
		</reference>
	</process_adminhtml_process_index>

	<process_adminhtml_column_index>
		<reference name="content">
			<block type="process/adminhtml_column_index" name="column_grid"/>
		</reference>
	</process_adminhtml_column_index>

	<process_adminhtml_entry_index>
		<reference name="content">
			<block type="process/adminhtml_entry_index" name="entry_grid"/>
		</reference>
	</process_adminhtml_entry_index>

	<process_adminhtml_process_edit>
		<reference name="content">
			<block type="process/adminhtml_process_edit" name="process_edit"/>
		</reference>
		<reference name="left">
			<block type="process/adminhtml_process_edit_tabs" name="process_tabs"/>
		</reference>
	</process_adminhtml_process_edit>

	<process_adminhtml_column_edit>
		<reference name="content">
			<block type="process/adminhtml_column_edit" name="column_edit"/>
		</reference>
		<reference name="left">
			<block type="process/adminhtml_column_edit_tabs" name="column_tabs"/>
		</reference>
	</process_adminhtml_column_edit>

	<process_adminhtml_group_edit>
		<reference name="content">
			<block type="process/adminhtml_group_edit" name="group_edit"/>
		</reference>
		<reference name="left">
			<block type="process/adminhtml_group_edit_tabs" name="group_tabs"/>
		</reference>
		<reference name="js">
			<block type="adminhtml/catalog_product_edit_js" template="catalog/product/js.phtml" name="catalog_product_js"></block>
            <block type="core/template" template="catalog/wysiwyg/js.phtml"/>
		</reference>
	</process_adminhtml_group_edit>

	<process_adminhtml_entry_edit>
		<reference name="content">
			<block type="process/adminhtml_entry_edit" name="entry_edit"/>
		</reference>
		<reference name="left">
			<block type="process/adminhtml_entry_edit_tabs" name="entry_tabs"/>
		</reference>
	</process_adminhtml_entry_edit>

	<process_adminhtml_upload_uploadfile>
        <reference name="content">
            <block type="process/adminhtml_process_upload_edit" name="process_upload_edit"></block>
        </reference>
        <reference name="left">
            <block type="process/adminhtml_process_upload_edit_tabs" name="process_upload_edit_tabs"></block>
        </reference>
    </process_adminhtml_upload_uploadfile>

    <process_adminhtml_upload_execute>
    	<reference name="content">
    		<block type="process/adminhtml_process_upload_progress" name="process_execute"></block>
    	</reference>
    </process_adminhtml_upload_execute>
</layout>